{
	"routes": {
		"index": {
			"path": "/",
			"action": "Index::index"
		},
		"featured": {
			"path": "/category/featured",
			"action": "Category::viewFeatured"
		},
		"categories": {
			"path": "/category/{categoryType}",
			"requirements": {
				"categoryType": "video|strategy|decks|judgement|ui"
			},
			"action": "Category::viewCategoryType"
		},
		"levels": {
			"path": "/category/{level}",
			"requirements": {
				"level": "beginner|intermediate|master"
			},
			"action": "Category::viewGuidesByLevel"
		},
		"view_guide": {
			"path": "/guide/{title}",
			"requirements": {
				"title": "[a-z0-9-]+"
			},
			"action": "Guide::viewGuide"
		},
		"view_series": {
			"path": "/series",
			"action": "Series::index"
		},
		"series_guide_list": {
			"path": "/series/{title}",
			"requirements": {
				"title": "[a-z0-9-]+"
			},
			"action": "Series::viewSeries"
		},
		"series_guide": {
			"path": "/series/{title}/{guideTitle}",
			"requirements": {
				"title": "[a-z0-9-]+",
				"guideTitle": "[a-z0-9-]+"
			},
			"action": "Guide::viewSeriesGuide"
		},
		"author": {
			"path": "/author/{author}",
			"requirements": {
				"author": "[a-zA-Z0-9_]+"
			},
			"action": "Author::viewGuides"
		},
		"vote_guide": {
			"path": "/vote/{guideUrl}/{voteType}",
			"requirements": {
				"guideUrl": "[a-z0-9-]+",
				"voteType": "up|down"
			},
			"action": "Vote::vote"
		},
		"submit_guide": {
			"path": "/submit",
			"action": "Guide::submit"
		},
		"save_guide": {
			"path": "/submit/save",
			"method": "POST",
			"action": "Guide::doSave"
		},
		"login": {
			"path": "/login",
			"action": "User::login"
		},
		"login_post": {
			"path": "/login",
			"action": "User::doLogin",
			"method": "POST"
		},
		"logout": {
			"path": "/logout",
			"action": "User::doLogout"
		},
		"view_avatar": {
			"path": "/assets/images/avatar/{user}",
			"requirements": {
				"user": "[a-zA-Z0-9-]+"
			},
			"action": "Resource::getAvatar"
		},
		"resource_cache": {
			"path": "/assets/cache/{filetype}/{filename}.{filetype}",
			"requirements": {
				"filetype": "css|js",
				"filename": "[a-zA-Z0-9-]+"
			},
			"action": "Resource::assetCache"
		},
		"about": {
			"path": "/about",
			"action": "Index::about"
		},
		"content_submission_guide": {
			"path": "/content-submission-guide",
			"action": "Index::contentSubmission"
		},
		"404": {
			"path": "/404",
			"action": "Index::p404"
		}
	}
}