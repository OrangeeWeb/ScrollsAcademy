{
	"routes": {
		"index": {
			"path": "/",
			"action": "Index::index"
		},
		"featured": {
			"path": "/category/featured",
			"action": "Category::viewFeatured"
		},
		"categories": {
			"path": "/category/{categoryType}",
			"requirements": {
				"categoryType": "video|strategy|decks|judgement|ui"
			},
			"action": "Category::viewCategoryType"
		},
		"levels": {
			"path": "/category/{level}",
			"requirements": {
				"level": "beginner|intermediate|master"
			},
			"action": "Category::viewGuidesByLevel"
		},
		"view_guide": {
			"path": "/guide/{title}",
			"requirements": {
				"title": "[a-z0-9-]+"
			},
			"action": "Guide::viewGuide"
		},
		"view_series": {
			"path": "/series",
			"action": "Series::index"
		},
		"series_guide_list": {
			"path": "/series/{title}",
			"requirements": {
				"title": "[a-z0-9-]+"
			},
			"action": "Series::viewSeries"
		},
		"series_guide": {
			"path": "/series/{title}/{guideTitle}",
			"requirements": {
				"title": "[a-z0-9-]+",
				"guideTitle": "[a-z0-9-]+"
			},
			"action": "Guide::viewSeriesGuide"
		},
		"view_avatar": {
			"path": "/assets/images/avatar/{user}",
			"requirements": {
				"user": "[a-zA-Z0-9-]+"
			},
			"action": "Resource::getAvatar"
		},
		"about": {
			"path": "/about",
			"action": "Index::about"
		},
		"404": {
			"path": "/404",
			"action": "Index::p404"
		}
	}
}