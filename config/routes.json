{
	"routes": {
		"index": {
			"path": "/",
			"action": "Index::index"
		},
		"featured": {
			"path": "/category/featured",
			"action": "Category::viewFeatured"
		},
		"categories": {
			"path": "/category/{categoryType}",
			"requirements": {
				"categoryType": "video|stragegy|decks|judgement|ui"
			},
			"action": "Category::viewCategoryType"
		},
		"levels": {
			"path": "/category/{level}",
			"requirements": {
				"level": "beginner|intermediate|expert"
			},
			"action": "Category::viewGuidesByLevel"
		},
		"view_guide": {
			"path": "/guide/{title}",
			"requirements": {
				"title": "[a-z-]+"
			},
			"action": "Guide::viewGuide"
		},
		"about": {
			"path": "/about",
			"action": "Index::about"
		},
		"404": {
			"path": "/404",
			"action": "Index::p404"
		}
	}
}